{
  "name": "PorTTY",
  "short_name": "PorTTY",
  "version": "0.2",
  "description": "Browser-based terminal access with persistent sessions.",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#000000",
  "theme_color": "#000000",
  "orientation": "any",
  "scope": "/",
  "categories": ["productivity", "utilities", "developer"],
  "lang": "en",
  "dir": "ltr",
  "icons": [
    {
      "src": "/icons/dark-theme-icon.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "any"
    }
  ],
  "screenshots": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "1280x720",
      "type": "image/svg+xml",
      "form_factor": "wide",
      "label": "PorTTY Terminal Interface"
    }
  ],
  "shortcuts": [
    {
      "name": "New Terminal",
      "short_name": "Terminal",
      "description": "Open a new terminal session",
      "url": "/",
      "icons": [
        {
          "src": "/icons/dark-theme-icon.svg",
          "sizes": "32x32",
          "type": "image/svg+xml"
        }
      ]
    }
  ],
  "prefer_related_applications": false,
  "protocol_handlers": [
    {
      "protocol": "ssh",
